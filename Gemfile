# A sample Gemfile
source "https://rubygems.org"

gem 'sinatra', :require => 'sinatra/base'
gem 'mysql2'
gem 'activerecord'
gem "activesupport", :require => "active_support"
gem 'sinatra-activerecord'
gem 'rake'

group :development do
  gem 'tux'
end

group :test do
  gem 'rspec'
  gem "minitest"
  gem 'rack-test'
end
